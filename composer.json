{
  "name": "youri/parser-turtle",
  "description": "Turtle (RDF 1.1) format handler for the Composer parser ecosystem.",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Youri van den Bogert"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "youri/parser-core": "dev-main"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Youri\\vandenBogert\\Software\\ParserTurtle\\": "src/"
    },
    "files": [
      "aliases.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest --colors=always --coverage-clover=coverage.xml",
    "analyse": "vendor/bin/phpstan analyse --no-progress"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
